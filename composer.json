{
    "name": "michaelferreira/symfony-factory",
    "description": "Factory for symphony entities",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael de Oliveira Ferreira",
            "email": "oliveiramichael@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "fzaninotto/faker": "1.9.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
